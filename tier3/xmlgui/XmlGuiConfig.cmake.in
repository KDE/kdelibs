@PACKAGE_INIT@

find_dependency(KCoreAddons "${PACKAGE_VERSION}")
find_dependency(KConfigWidgets "${PACKAGE_VERSION}")
find_dependency(Qt5DBus @QT_REQUIRED_VERSION@)
find_dependency(Qt5Xml @QT_REQUIRED_VERSION@)


include("${CMAKE_CURRENT_LIST_DIR}/XmlGuiTargets.cmake")

set(XmlGui_LIBRARIES KF5::XmlGui)
set(XmlGui_INCLUDE_DIRS ${XmlGui_INCLUDE_DIR} )

# XmlGui optionally adds LibAttica::attica to its public link interface
set(XmlGUI_NEEDS_ATTICA @HAVE_ATTICA@)
if(XmlGUI_NEEDS_ATTICA)
  include(FeatureSummary)
  find_package(LibAttica @LibAttica_MIN_VERSION@ CONFIG)
  set_package_properties(LibAttica PROPERTIES TYPE REQUIRED
                                              PURPOSE "Support for Get Hot New Stuff in XMLGUI"
                        )
endif()
