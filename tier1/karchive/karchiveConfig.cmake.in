@PACKAGE_INIT@

# Any changes in this ".cmake" file will be overwritten by CMake, the source is the ".cmake.in" file.

set(karchive_VERSION_MAJOR "@KARCHIVE_VERSION_MAJOR@")
set(karchive_VERSION_MINOR "@KARCHIVE_VERSION_MINOR@")
set(karchive_VERSION_PATCH "@KARCHIVE_VERSION_PATCH@")

set(karchive_INSTALL_PREFIX "@PACKAGE_CMAKE_INSTALL_PREFIX@")
set_and_check(karchive_INCLUDE_DIR    "@PACKAGE_INCLUDE_INSTALL_DIR@")

set(karchive_HAVE_BZIP2 "@BZIP2_FOUND@")
set(karchive_HAVE_LZMA  "@LIBLZMA_FOUND@")

if(NOT TARGET KF5::karchive)
