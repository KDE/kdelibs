  <refsynopsisdiv>
    <cmdsynopsis>
      <command>&dhpackage;</command>

      <arg><option>--exit</option></arg>
      <arg><option>--no-dcop</option></arg>
      <arg><option>--no-kded</option></arg>
      <arg><option>--no-klauncher</option></arg>
