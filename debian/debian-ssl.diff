+  if [ -x /usr/sbin/update-alternatives ]; then
+    /usr/sbin/update-alternatives --remove kio_https.la /usr/lib/kde2/kio_https-ssl.la
+    /usr/sbin/update-alternatives --remove kio_https.so /usr/lib/kde2/kio_https-ssl.so
+    /usr/sbin/update-alternatives --remove libkssl.so.2.0.2 /usr/lib/libkssl-ssl.so.2.0.2
