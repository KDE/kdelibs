+  fi
+  dpkg-divert --package kdelibs3-crypto --add --rename \
+    --divert /usr/lib/kde2/kio_https-nossl.la /usr/lib/kde2/kio_https.la
+  dpkg-divert --package kdelibs3-crypto --add --rename \
+    --divert /usr/lib/kde2/kio_https-nossl.so /usr/lib/kde2/kio_https.so
+  dpkg-divert --package kdelibs3-crypto --add --rename \
+    --divert /usr/lib/libkssl-nossl.so.2.0.2 /usr/lib/libkssl.so.2.0.2
+  dpkg-divert --package kdelibs3-crypto --add --rename \
+    --divert /usr/lib/libkssl-nossl.la /usr/lib/libkssl.la
+    ;;
+ 
+    abort-upgrade)
+    ;;
+ 
+    *)
+        echo "preinst called with unknown argument \`$1'" >&2
+        exit 0
+    ;;
+esac
+
+#DEBHELPER#
+
+exit 0
